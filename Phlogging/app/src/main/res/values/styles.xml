<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CameraGalleryButton">
        <item name="android:layout_height">@dimen/button_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">@dimen/button_text</item>
        <item name="android:onClick">myClickHandler</item>
    </style>
    <style name="EditUIEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/edit_text</item>
    </style>
    <style name="EditUIDataText">
        <item name="android:textSize">@dimen/data_text</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SaveDeleteButton">
        <item name="android:layout_height">@dimen/button_height</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:textSize">@dimen/button_text</item>
        <item name="android:layout_weight">@integer/button_weight</item>
        <item name="android:onClick">myClickHandler</item>
    </style>
</resources>